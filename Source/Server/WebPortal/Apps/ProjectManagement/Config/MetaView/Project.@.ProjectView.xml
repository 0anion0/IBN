				<Top>50</Top>
				<Drag>True</Drag>
			</Command>
			<Command id="MC_PM_Managers">
				<CommandType>OpenFrameModalPopup</CommandType>
				<PopupTitle>{IbnFramework.Project:PrjMans}</PopupTitle>
				<Url>~/Projects/EditManagers.aspx?ProjectId={QueryString:ProjectId}</Url>
				<Width>380</Width>
