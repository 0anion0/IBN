				<CommandType>OpenFrameModalPopup</CommandType>
				<PopupTitle>{IbnFramework.Global:ChangeFilter}</PopupTitle>
				<Url>~/Projects/ResourceUtilFilter.aspx?ProjectId=[ClientParam:ProjectId]&amp;Objs=[ClientParam:Objs]</Url>
				<Width>700</Width>
				<Height>450</Height>
				<Top>15</Top>
				<Left>15</Left>
				<Drag>True</Drag>
			</Command>

