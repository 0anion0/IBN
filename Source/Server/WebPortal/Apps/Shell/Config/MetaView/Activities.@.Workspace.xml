				<Column id="colManager" type="default" width="140px" title="{IbnFramework.Project:Manager}" sortExpression="ManagerId" templateUrl="~/Apps/Shell/ColumnTemplates/ToDo.ManagerStatus.ascx" />
				<Column id="colFinishDate" type="default" width="80px" title="{IbnFramework.Task:DueDate}" sortExpression="FinishDate" templateUrl="~/Apps/Shell/ColumnTemplates/ToDo.FinishDate.ascx" />
			</CustomColumns>
		</add>
	</Grid>
	<Commands>
	</Commands>
</MetaView>